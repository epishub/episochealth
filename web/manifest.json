


{
    "name": "sochealthepstral",
    "description": "Chrome extension to understand your consumption habits",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "content_security_policy": {
        "extension_pages": "script-src 'self' ; object-src 'self'"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["contextMenus", "clipboardWrite", "clipboardRead"],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "favicon.png",
            "32": "icons/Icon-192.png",
            "48": "icons/Icon-maskable-192.png",
            "128": "icons/Icon-512.png"
        }
    },
    "icons": {
        "16": "favicon.png",
        "32": "icons/Icon-192.png",
        "48": "icons/Icon-maskable-192.png",
        "128": "icons/Icon-512.png"
    }
}